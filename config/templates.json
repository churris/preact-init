{
	"validPackage": {
		"private": true,
		"scripts": {
			"start": "preact-roller start",
			"build": "preact-roller build"
		},
		"eslintConfig": {
    	"extends": "./node_modules/preact-roller/config/.eslintrc"
  	}
	},
	"dependencies": [
		"preact",
		"preact-compat"
	],
	"devDependencies": [
		"preact-roller"
	],
	"validFiles": [
		".DS_Store",
		"Thumbs.db",
		".git",
		".gitignore",
		".idea",
		"README.md",
		"LICENSE",
		"web.iml"
	],
	"requiredTree": {
		"children": [
			{
				"name": "package.json",
				"extension": ".json"
			},
			{
				"name": "public",
				"children": [
					{
						"name": "index.html",
						"extension": ".html"
					}
				]
			},
			{
				"name": "src",
				"children": [
					{
						"name": "index.jsx",
						"extension": ".jsx"
					}
				]
			}
		]
	}
}
